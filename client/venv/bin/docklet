#!/home/augustin/docklet/client/venv/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'docklet','console_scripts','docklet'
__requires__ = 'docklet'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('docklet', 'console_scripts', 'docklet')()
    )
